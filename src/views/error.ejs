<% layout('layout') -%>

<div class="row mt-5">
  <div class="col-6 offset-3">
    <div class="alert alert-danger" role="alert">
      <p class="alert-heading"><b><%= err.message %></b></p>

      <% if (process.env.NODE_ENV !== "production") { %>
      <hr />
      <p class="mb-0"><%= err.stack %></p>
      <% } %>
    </div>
    <a href="/listings" class="btn btn-dark">Back to Home</a>
  </div>
</div>
